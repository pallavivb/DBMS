CREATE TABLE FLAT(
FLAT_NO NUMBER(5) PRIMARY KEY NOT NULL,
F_TYPE VARCHAR(10),
REG_NO VARCHAR(20)
);
CREATE TABLE COMMITTEE(
CMEMBERID VARCHAR(5) PRIMARY KEY,
COM_NAME VARCHAR(20),
ROLE VARCHAR(20)
);
CREATE TABLE OWNER(
OWNER_ID VARCHAR(5) PRIMARY KEY,
FLAT_NO NUMBER(5) REFERENCES FLAT(FLAT_NO) ON DELETE CASCADE,
NAME VARCHAR(20),
PARKING_SLOT VARCHAR(5),
CMEMBERID VARCHAR(5) REFERENCES COMMITTEE(CMEMBERID) ON DELETE CASCADE
);
CREATE TABLE TENANT
